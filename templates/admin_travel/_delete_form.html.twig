<form method="post" action="{{ path('app_admin_travel_delete', {'id': travel.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ travel.id) }}">
    <button class="btn btn-danger border-danger">Supprimer<i class="bi bi-trash3-fill ms-2"></i></button>
</form>
